{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "VersionProposalTemplate",
  "description": "升级提案",
  "type": "object",
  "properties": {
    "verifier": {
      "description": "提交提案的验证人，nodeId",
      "type": "string"
    },
    "piPid": {
      "description": "PIPID",
      "type": "string"
    },
    "endVotingRound": {
      "description": "投票共识轮数量",
      "type": "integer",
      "minimum": 0
    },
    "newVersion": {
      "description": "升级版本",
      "type": "integer",
      "minimum": 0
    }
  },
  "required": [
    "verifier",
    "piPid",
    "endVotingRound",
    "newVersion"
  ]
}